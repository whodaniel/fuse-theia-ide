  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#64748b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#a855f7"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#f8fafc"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ec4899"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#fbbf24",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#ef4444"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#a855f7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#a855f7"
      }
    },
    {
      "name": "Decorators",
      "scope": ["meta.decorator"],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ec4899"
      }
    }
  ]
}
